#!/bin/sh
###########################################################
# Purpose:
# 	Do IPTA for the given area.
# Calling Sequence:
# 	./IPTA_TJ_TSX.sh
# Inputs:
# 	None
# Optinal Input Parameters:
# 	None
# Outputs:
# 	IPTA results that can be checked by using some professional tools.
# Commendations:
# 	None
# Modification History:
# 	24/02/2012: Written by T.L @ InSAR Group in SWJTU & CUHK.
######################################
### Set some parameters
######################################
origpath="/mnt/ihiusa/Software/Data/SLCs/TSX_TJ_Coregto20091113"                 #原始SLC所在路径
dem="/mnt/backup/DEM/TJ_UTM_DEM"                     #原始DEM所在路径
path="/mnt/backup/ForExperiment/TSX_PS_Tianjin_FullScene/piece"     #裁剪之后的SLC所在路径
master="20091113"
masterpwr=$path/$master.rslc.pwr
mslc_par=$path/$master.rslc.par
# set the cropping parameters
roff="200"      #roff: offset to starting range sample
nr="14970"       #nr: number of range samples
loff="1840"    #loff: offset number of lines to start
nl="27950"       #nl: number of lines to copy (enter 0: to end of file)
itab_type=0     #0: single master;  1: all pairs
dem_par=$dem.par
####################################################################
# 1. 准备SLC数据
####################################################################
if false
then
	mkdir -p $path
	rm -f SLC_org im_list SLC_tab
	find $origpath -name "*.rslc" -follow -exec echo {} {}.par >>SLC_org \;
	SLC_copy_all SLC_org $path $roff $nr $loff $nl
	find $path -name "*.rslc" -exec multi_look {} {}.par {}.pwr {}.pwr.par 1 1 \; #注意多视参数
	find $path -name "*.rslc" -exec echo {} {}.par >>SLC_tab \;
	find $path -name "*.pwr" -exec echo {} >>im_list \;
fi

# There is not enough space to process the average image using .pwr files. 
# However, I still need to check all the cropped slc images.
# Using the rasSLC command.
find $path -name "*.rslc" -exec rasSLC {} 14970 - - 10 10 - - - 1 \;


	width_mli=$(awk '$1 == "range_samples:" {print $2}' $masterpwr.par)
	line_mli=$(awk '$1 == "azimuth_lines:" {print $2}' $masterpwr.par)
	width=$(awk '$1 == "range_samples:" {print $2}' $mslc_par)
	line=$(awk '$1 == "azimuth_lines:" {print $2}' $mslc_par)
	type=0
	format=$(awk '$1 == "image_format:" {print $2}' $mslc_par)
	if [ "$format" == "SCOMPLEX" ]
	then
		type=1
	fi

	ave_image im_list $width ave.pwr  #ave.pwr
	mv $masterpwr.par .
	mv $masterpwr .
	rm -f $path/*.pwr $path/*.pwr.par
	mv $master.rslc.pwr.par $path/
	mv $master.rslc.pwr $path/
	raspwr ave.pwr $width - - - - - - - ave.ras  #ave.ras	

####################################################################
# 2. 准备高程数据
####################################################################

	rm dem_seg dem_seg.par
	gc_map $masterpwr.par - $dem_par $dem dem_seg.par dem_seg lookup 5 5 sim_sar  # Use disdem_par to look over dem_seg.
	# 注意保证DEM的正确性。特别是投影类型。
	# disdem_par dem_seg dem_seg.par - - - - -
	width_dem=$(awk '$1 == "width:" {print $2}' dem_seg.par)
	nlines_map=$(awk '$1 == "nlines:" {print $2}' dem_seg.par)
	col_post=$(awk '$1 == "post_north:" {print $2}' dem_seg.par)
	row_post=$(awk '$1 == "post_east:" {print $2}' dem_seg.par)
	#rasshd dem_seg $width_dem $col_post $row_post - - - - - - - dem_seg.ras
	geocode lookup sim_sar $width_dem sim_sar.rdc $width_mli $line_mli 1 0
	raspwr sim_sar.rdc $width_mli 1 0 1 1 1. .35 1 sim_sar.rdc.ras
	echo -ne "$M_P-$S_P\n 0 0\n 32 32\n 64 64\n 7.0\n 0\n\n" > create_diffpar
	create_diff_par $masterpwr.par - $master.diff_par 1 <create_diffpar
	rm -f create_diffpar
	offset_pwrm sim_sar.rdc ave.pwr $master.diff_par $master.offs $master.snr - - offsets 2
	offset_fitm $master.offs $master.snr $master.diff_par coffs coffsets - -
	offset_pwrm sim_sar.rdc ave.pwr $master.diff_par $master.offs $master.snr - - offsets 2 24 24 -
	gc_map_fine lookup $width_dem $master.diff_par lookup_fine 1 #Fine look up table
	geocode_back ave.pwr $width_mli lookup_fine ave.utm.rmli $width_dem - 3 0  #Geocoded pwr
	raspwr ave.utm.rmli $width_dem - - - - - - - ave.utm.rmli.ras ##运行第二遍的时候，吐核了
	#geocode lookup_fine dem_seg $width_dem $master.hgt $width - 0 0
	rm $master.hgt
	geocode lookup_fine dem_seg $width_dem $master.hgt $width - 1 0
	rashgt $master.hgt ave.pwr $width - - - - - 160. - - - $master.hgt.ras #hgt & pwr

####################################################################
# 3. 计算时空基线，并生成itab文件
#####################################################################
	base_calc SLC_tab $mslc_par base_plot.agr base.list itab $itab_type
	# xmgrace base_plot.agr #画基线图


	width_mli=$(awk '$1 == "range_samples:" {print $2}' $masterpwr.par)
	line_mli=$(awk '$1 == "azimuth_lines:" {print $2}' $masterpwr.par)
	width=$(awk '$1 == "range_samples:" {print $2}' $mslc_par)
	line=$(awk '$1 == "azimuth_lines:" {print $2}' $mslc_par)
	type=0
	format=$(awk '$1 == "image_format:" {print $2}' $mslc_par)
	if [ "$format" == "SCOMPLEX" ]
	then
		type=1
	fi

if true
then
######################################################################
# 4. 生成点位列表文件
######################################################################
	# bad scripts.
	#sp_stat $path/$master.rslc - $master.cc $master.msr $master.pt $width 0.0 0.4 1.0 4 4 - - - - - - $type - #对单一影像进行统计分析
	#mkdir sp_dir
	#mk_sp_all SLC_tab sp_dir 1 1 0.5 0.4 4 #对全部影像进行统计分析
	#single_class_mapping 2 sp_dir/ave.sp_cc 0 100 sp_dir/ave.sp_msr 2 100. classification.ras $width 1 0 1 1
	#image2pt classification.ras $width pt - - 6
	pwr_stat SLC_tab $mslc_par msr pt 2 1 - - - - $type 0
	ras_pt pt - ave.ras pt.ras - - 255 255 0 3

######################################################################
# 5. Create pSLC_par and pSLC data stacks
#	Estimate the baseline parameters from orbit state vectors
######################################################################
	SLC2pt SLC_tab pt - pSLC_par pSLC -
	base_orbit_pt pSLC_par itab - pbase
	# base_par_pt - pbase - 0
fi


######################################################################
# 6. Extract height values for each target point
######################################################################
npt pt >numberp
np=$(awk '$1 == "total_number_of_points:" {print $2}' numberp)
rm -f numberp
if true
then

	rm -f pdem
	# 如有问题，检查参数，某些参数用-来代替会出现问题
	data2pt $master.hgt $masterpwr.par pt $mslc_par pdem 1 2
	# pdisdt_pwr24 pt - $mslc_par pdem 1 $masterpwr.par ave.pwr 256. 1
	
#####################################################################
# 8. Compute differential phase values for each target point 
#    from each InSAR pair
######################################################################
	rm -f pint
	intf_pt pt - itab - pSLC pint $type pSLC_par
	# pdismph_pwr24 pt - $mslc_par pint 25 $masterpwr.par ave.pwr 1 #显示第25幅影像的干涉图
	rm -f psim_unw0
	phase_sim_pt pt - pSLC_par - itab - pbase pdem psim_unw0 - 0 0 #############注意，相位模拟可能存在问题
	rm -f pdiff0
	sub_phase_pt pt - pint - psim_unw0 pdiff0 1 0
	prasmph_pwr24 pt - $mslc_par pdiff0 - $masterpwr.par ave.pwr 1 # Produce the 3rd different interferogram of the pt. Named as pdiff.ras.
	dis_ipta pt - pSLC_par - itab pbase 0 pdiff0 1 pdiff0.22.ras 30 -0.01 0.01 2 # #########版本不同，命令不同，注意参数的设置
	# dis_ipta pt - pSLC_par - itab pbase 0 pdiff0 1 ras/pdiff/pdiff0.22.ras 30 -0.01 0.01 2 
	mkdir -p ras/pdiff
	mv pdiff*ras ras/pdiff
	#bpf_pt pt - $mslc_par pdiff pdiff1 - 0 $mslc_par 0.0 0.05# Filterring, not suggested.
	exit
fi



######################################################################
# 9. Determine height corrections, deformation rates and residual phases 
#    by the LS-based model regression
#    Note: the reference-point number should be carefully chosen.
#	The point number of the reference point can be indicated. To minimize spactial distances it is usually adequate to select a reference point near the center of the area of interest. On the other hand it is often preferred to select a point known to be stable or known to be a reference point of a leveling network, as reference. To identify the point number of a point at (near) the desired coordinate the programs dis_ipta or prox_pt can be used.
######################################################################
if false
then
	refpsno=62186 # The reference point number.
	rm -f pres1 pdh1 pddef1 punw1 psigma1 pmask1 pdh1
	#multi_def_pt pt - pSLC_par - itab pbase 0 pdiff 1 $refpsno pres pdh pddef punw1 psigma pmask 60 -0.01 0.01 1000 1.5 2 2 0 - - # sigma=1.5
#usage: multi_def_pt <pt> <pmask_in> <pSLC_par> <ppos> <itab> <pbase> <bflag> <pdiff> <pdiff_type> <np_ref> <pres> <pdh> <pdef> <punw> <psigma> <pmask_out> [dh_max] [def_min] [def_max] [rpatch] [sigma_max] [sigma_max2] [model] [noise_min] [bmax] [dtmax]
#usage: def_mod_pt <pt> <pmask_in> <pSLC_par> <ppos> <itab> <pbase> <bflag> <pdiff> <pdiff_type> <np_ref> <pres> <pdh> <pdef> <punw> <psigma> <pmask_out> [dh_max] [def_min] [def_max] [sigma_max] [model] [pdh_err] [pdef_err] [ppc_err] [bmax] [dtmax]
	#def_mod_pt pt - pSLC_par - itab pbase 0 pdiff 1 $refpsno pres pdh pddef punw1 psigma pmask 60 -0.01 0.01 1.2 2 pdh_err pddef_err ppc_err #####1899
	#def_mod_pt pt - pSLC_par - itab pbase 0 pdiff 1 $refpsno pres pdh pddef punw1 psigma pmask 60 -0.1 0.1 1.2 2 pdh_err pddef_err ppc_err #####1901
	#def_mod_pt pt - pSLC_par - itab pbase 0 pdiff 1 $refpsno pres pdh pddef punw1 psigma pmask 600 -0.01 0.01 1.2 2 pdh_err pddef_err ppc_err ##### 耗时太长，中途退出
	#multi_def_pt pt - pSLC_par - itab pbase 0 pdiff 1 $refpsno pres pdh pddef punw1 psigma pmask 20 -0.006 0.006 300 1.35 0.9 2 #1166点，集中在一块
	# multi_def_pt pt - pSLC_par - itab pbase 0 pdiff 1 $refpsno pres pdh pddef punw1 psigma pmask 20 -0.006 0.006 600 1.35 0.9 2 #2883点，集中在一块
	#multi_def_pt pt - pSLC_par - itab pbase 0 pdiff 1 $refpsno pres pdh pddef punw1 psigma pmask 20 -0.006 0.006 1000 1.35 0.9 2 #6904点，集中在一块
	#multi_def_pt pt - pSLC_par - itab pbase 0 pdiff 1 $refpsno pres pdh pddef punw1 psigma pmask 20 -0.006 0.006 1000 1.5 0.9 2 #19503。。。
	#multi_def_pt pt - pSLC_par - itab pbase 0 pdiff 1 $refpsno pres pdh pddef punw1 psigma pmask 20 -0.006 0.006 1000 2.0 0.9 2 #39309。。。
	#multi_def_pt pt - pSLC_par - itab pbase 0 pdiff0 1 $refpsno pres1 pdh1 pddef1 punw1 psigma1 pmask1 20 -0.006 0.006 600 2.0 2.0 2	#318069
	multi_def_pt pt - pSLC_par - itab pbase 0 pdiff0 1 $refpsno pres1 pdh1 pddef1 punw1 psigma1 pmask1 30 -0.01 0.01 100 1.2 1.0 2 0 500	#318069
	ras_pt pt pmask1 ave.ras ave_mask_pt.ras - - - - - 5
	# bpf_pt pt pmask2 $mslc_par pdef pdef1 - 2 $mslc_par 0.0 0.05# Filtering, not suggested.
	# bpf_pt pt pmask2 $mslc_par pdh pdh1 - 2 $mslc_par 0.0 0.05# Filtering, not suggested.

######################################################################
# 10. Start iterativation. If you do not meed the precise results.
#     Just set FALSE to jump this step.
#     Attention: There may be some stop points during this step.
######################################################################
	prasmph_pwr24 pt - $mslc_par pdiff0 3 $masterpwr.par ave.pwr 1
	prasdt_pwr24 pt pmask1 $mslc_par pdh1 - $masterpwr.par ave.pwr 30.0 1
	prasdt_pwr24 pt pmask1 $mslc_par pddef1 1 $masterpwr.par ave.pwr 0.01 1
	prasdt_pwr24 pt pmask1 $mslc_par psigma1 1 $masterpwr.par ave.pwr 1.5 1
	prasdt_pwr24 pt pmask1 $mslc_par pres1 - $masterpwr.par ave.pwr 12.6 1  #pres1中，有些干涉对只有参考点有值。这是由于multi_def_pt中的基线限制造成的。
	
	cp itab itab.selection1
	xv -quit
	echo '*********************************************************************'
	echo '*                   This is the first stop point                    *'
	echo '*   Please modify the itab.selection1 file according to residuals   *'
	echo '*********************************************************************'
	echo ""
	echo "Press anykey to quit."
	read temp
	gedit itab.selection1
	exit
fi
###改变itab，去除残差不正确的影像对，再次迭代。自此步骤之后，itab中mask为0的影像对，其值为0，影响到punw1,pres2,pdh2......，务必注意。


refpsno=62186
if false
then
	mkdir residual
	mv pres1.* residual/	
	#def_mod_pt pt - pSLC_par - itab.selection1 pbase 0 punw1 0 $refpsno pres2 pdh2 pddef2 punw2 psigma2 pmask2 85. 0.03 3.0 1.2 - - - # sigma=4
	#def_mod_pt pt - pSLC_par - itab.selection1 pbase 0 punw1 0 $refpsno pres2 pdh2 pddef2 punw2 psigma2 pmask2 85. -0.03 0.03 1.2 - - - # 1946
	def_mod_pt pt pmask1 pSLC_par - itab.selection1 pbase 0 punw1 0 $refpsno pres2 pdh2 pddef2 punw2 psigma2 pmask2 25 -0.03 0.03 3.0 - - - #192492
	#ras_pt pt pmask2 ave.ras ave_mask_pt.ras - - - - - 5

	rm -fr residual
	#prasdt_pwr24 pt pmask1 $mslc_par pdh2 1 $masterpwr.par ave.pwr 30.0 1
	#prasdt_pwr24 pt pmask1 $mslc_par pddef2 1 $masterpwr.par ave.pwr 0.01 1
	#prasdt_pwr24 pt pmask1 $mslc_par pddef2 1 $masterpwr.par ave.pwr 0.003 1
	#prasdt_pwr24 pt pmask1 $mslc_par pddef2 1 $masterpwr.par ave.pwr 0.005 1
	#prasdt_pwr24 pt pmask1 $mslc_par psigma2 1 $masterpwr.par ave.pwr 1.5 1
	#prasdt_pwr24 pt pmask1 $mslc_par pres2 - $masterpwr.par ave.pwr 12.6 1
	


# 最初始的结果已经计算完毕。pdh2可以用来修正pdem，pddef2提供了形变场信息，pres2包含各干涉图的相位残差。
 # Update pdem using pdh

	lin_comb_pt pt pmask2 pdem 1 pdh2 1 phgt1 1 0. 1. 1. 2 0
	
	ras_data_pt pt - phgt1 1 - ave.ras phgt1.ras 2 1 1 5.0 3
	
	# Refine the result using phgt1&pddef2
	cp pddef2 pdef1
	phase_sim_pt pt pmask2 pSLC_par - itab - pbase phgt1 psim_unw0 pdef1 0 0
	sub_phase_pt pt pmask2 pint - psim_unw0 pdiff0 1 0 #消除了长基线带来的失相关
	#prasmph_pwr24 pt pmask2 $mslc_par pdiff0 - $masterpwr.par ave.pwr 1 # Produce the 3rd different interferogram of the pt. Named as pdiff.ras.

## 使用精化的高程，再次迭代

	rm -f pres1 pdh1 pddef1 punw1 psigma1 pmask1
	#multi_def_pt pt - pSLC_par - itab pbase 0 pdiff0 1 $refpsno pres1 pdh1 pddef1 punw1 psigma1 pmask1 100 -1 1 800 50 6 2 1 #sigma=1.3
	#multi_def_pt pt - pSLC_par - itab pbase 0 pdiff0 1 $refpsno pres1 pdh1 pddef1 punw1 psigma1 pmask1 3 -0.003 0.003 100 1.3 0.9 1 #389718
	multi_def_pt pt - pSLC_par - itab pbase 0 pdiff0 1 $refpsno pres1 pdh1 pddef1 punw1 psigma1 pmask1 3 -0.003 0.003 100 1.3 0.9 2 1
# 上面已经去除了大部分高程误差和残差，因此高程误差和sigma的值均设置的比较小。

# punw1居然是10e-38数量级？
	#ras_pt pt pmask1 ave.ras ave_mask_pt.ras - - - - - 5

	#prasdt_pwr24 pt pmask1 $mslc_par pdh1 1 $masterpwr.par ave.pwr 10.0 1
	#prasdt_pwr24 pt pmask1 $mslc_par pddef1 1 $masterpwr.par ave.pwr 0.01 1
	#prasdt_pwr24 pt pmask1 $mslc_par psigma1 1 $masterpwr.par ave.pwr 1.5 1
	prasdt_pwr24 pt pmask1 $mslc_par pres1 - $masterpwr.par ave.pwr 12.6 1 #包含解缠误差的点已急剧减少
	mkdir residual
	mv pres2*.ras residual/
	
	
	cp itab itab.selection2
	echo '*********************************************************************'
	echo '*                   This is the second stop point                   *'
	echo '*        Do the iteration once again in this step if you like       *'
	echo '*                    BUT it is NOT very neccesary                   *'
	echo '*********************************************************************'
	echo ""
	echo 'Please check the residuals. And find out record NO. with bad residual phase.'
	echo 'Do use a pencil to write down the record NO.'
	echo "Press anykey to continue:"
	read temp
	rm -f residual/*
	mv pres*.ras residual/
	exit
fi



######################################################################
# 11. 相位解缠，某些图层依然可能解缠失败，其原因多为轨道残差和大气的影响。
######################################################################
if false
then
	rps=$(awk '$1 == "range_pixel_spacing:" {print $2}' $mslc_par)
	azps=$(awk '$1 == "azimuth_pixel_spacing:" {print $2}' $mslc_par)
	unw_to_cpx_pt pt pmask1 pres1 - pres1.cpx
	mcf_pt pt pmask1 pres1.cpx - - - pres1.cpx.unw $rps $azps $refpsno 0
	prasdt_pwr24 pt pmask1 $mslc_par pres1.cpx.unw - $masterpwr.par ave.pwr 12.6 1 # Disp unwrapping result for the 5th interferogram.

######################################################################
# 12. Calculate the phase regression model.
#	psim_unw2	:	Simulated phase of interferogram. 
#				Which contains no deformation info.
#	punw1    	:	Unwrapped phase on points in time-series.
#	ptmp1    	:	Spatio-time unwrapping result 
#				without deformation info.
#	pres1.cpx.unw :	Residual phase(unwrapped).
#	ptmp2		:	All spatio-time phase components without deformation.
#	pdiff0_unw	:	Unwrapped phase which contains two components:
#				Flattenning
#				DEM
######################################################################

	#lin_comb_pt pt pmask1 phgt1 - pdh1 - phgt2 - -0. 1. 1. 2 0
	#mv phgt2 phgt1



#############################问题所在 #############################
	#模拟空间维的解缠相位。共包括四个部分：
	#1）模拟形变场+高程误差
	#2）
# psim_unw2中，一部分相位为0，需要用lin_comb_pt添加此部分数据。
	#phase_sim_pt pt - pSLC_par - itab - pbase phgt psim_unw - 0 0
	#phase_sim_pt pt pmask2 pSLC_par - itab - pbase phgt1 psim_unw0 pdef1 0 0
	#phase_sim_pt pt pmask1 pSLC_par - itab - pbase phgt1 psim_unw2 pddef1 2 0
	phase_sim_pt pt pmask1 pSLC_par - itab - pbase pdh1 psim_unw2 pddef1 2 0
#############################问题所在 #############################
	prt_pt pt pmask1 punw1 $refpsno 1 2 - 1 56 # Print point info. #打印点位信息，可以用来做图。
	#注意lin_comb_pt的版本
	rm ptmp1 ptmp2
	lin_comb_pt pt pmask1 psim_unw2 - punw1 - ptmp1 - 0. 1. 1. 2 1 $refpsno # 只在参考点上相加？？？ptmp1:把psim_unw2上参考点的相位加到punw1上。
	#prt_pt pt - psim_unw2 $refpsno 1 2 $refpsno-psim_unw2.txt 1
	#prt_pt pt - punw1 $refpsno 1 2 $refpsno-punw1.txt 1
	#prt_pt pt - ptmp1 $refpsno 1 2 $refpsno-ptmp1.txt 1
	#gedit $refpsno-psim_unw2.txt $refpsno-punw1.txt $refpsno-ptmp1.txt

	lin_comb_pt pt pmask1 ptmp1 - pres1.cpx.unw - ptmp2 - 0. 1. 1. 2 1 # 将不含高程误差的残差加到ptmp1中。
	#prt_pt pt - ptmp1 $refpsno 1 2 $refpsno-ptmp1.txt 1
	#prt_pt pt - ptmp2 $refpsno 1 2 $refpsno-ptmp2.txt 1
	#prt_pt pt - pres1.cpx.unw $refpsno 1 2 $refpsno-pres1.cpx.unw.txt 1
	#gedit $refpsno-ptmp1.txt $refpsno-pres1.cpx.unw.txt $refpsno-ptmp2.txt 
	
	masterind=$(awk "/$master.rslc.par/ {print NR}" SLC_tab)

	lin_comb_pt pt pmask1 ptmp2 - pres1.cpx.unw $masterind pdiff0_unw - 0. 1. -1. 2 1 # pdiff0_unw:  ptmp2-残差
	#注意，此处应根据个人情况修改。上面pres1.cpx.unw对应的序号，是主影像的序号。
	pdis2dt pt pmask1 $mslc_par punw1 20 pdiff0_unw 20 $masterpwr.par 12.6 0
	#pdis2dt pt pmask1 $mslc_par punw1 23 pdiff0_unw 23 $masterpwr.par 12.6 0

# 查看两者的差异. punw1:时间维解缠。pdiff0_unw:空间维解缠。
	rm ptmp
	lin_comb_pt pt pmask1 punw1 - pdiff0_unw - ptmp - 0. 1. -1. 2 1
	prasdt_pwr24 pt pmask1 $mslc_par ptmp - $masterpwr.par ave.pwr 12.6 1
	cp pmask1 pmask1_tmp
	thres_msk_pt pt pmask1_tmp ptmp 23 -0.1 0.1
	rm -fr ptmp_ras
	
	echo '*********************************************************************'
	echo '*                   This is the third stop point                    *'
	echo '*	You need to modify those images with bad residual phase     *'
	echo '*	Their indices should have already been written down         *'
	echo '*	by yourself at the end of step10(Stop point 2)              *'
	echo '*********************************************************************' 
	echo 'Press anykey to continue:'
	read temp
	exit
fi




if false
then
# 将改正之后的干涉对添加到数据堆中。有问题有问题阿****************************************************************
#注意，这一步有错误，刚更正完。20120627 1657
# 此处需要改正的，是含有轨道误差的部分。不是第一次修改itab的时候发现的含有大气的影像对。
	cp punw1 punw1a
	lin_comb_pt pt pmask1 pdiff0_unw 14 pdiff0_unw 14 punw1a 14 0. 1 0 2 1
	lin_comb_pt pt pmask1 pdiff0_unw 21 pdiff0_unw 21 punw1a 21 0. 1. 0. 2 1
	lin_comb_pt pt pmask1 pdiff0_unw 25 pdiff0_unw 25 punw1a 25 0. 1. 0. 2 1
	lin_comb_pt pt pmask1 pdiff0_unw 36 pdiff0_unw 36 punw1a 36 0. 1. 0. 2 1
	lin_comb_pt pt pmask1 pdiff0_unw 37 pdiff0_unw 37 punw1a 37 0. 1. 0. 2 1
	lin_comb_pt pt pmask1 pdiff0_unw 30 pdiff0_unw 30 punw1a 30 0. 1. 0. 2 1
	mkdir ptmp_ras
	mv ptmp*.ras ptmp_ras/

	phase_sim_pt pt pmask1 pSLC_par - itab - pbase phgt1 psim_unw0 pdef1 0 0
	sub_phase_pt pt pmask1 punw1a - psim_unw0 pint_unw 0 1 # 获取足够精确的解缠结果。


##############################################################
# 12. 基线精化。选用稳定点，使用DEM提供的原始高程对所有干涉对进行基线精化。
##############################################################
################重要步骤，使用精确的解缠结果进行基线精化。
	cp pmask1 pdef1_mask
	thres_msk_pt pt pdef1_mask pdef1 1 -0.002 0.002
	# 一定要选取形变较小的点，以减少形变的影响。（形变，大气，轨道在图像上的行为较为一致）
	cp pbase pbase.orbit
	base_ls_pt pt pdef1_mask pSLC_par - itab - pint_unw pdem pbase 0 1 1 1 1 1 >> base_ls_pt.out
	#grep "b_perp (t=center) (m):" base_ls_pt.out
	#grep "RMS altitude error (m):" base_ls_pt.out
	cp pbase pbase.120830
	# Update hight & deformation value according to 
	# unwrapped phase and refined baseline
	# 使用改进的基线进行干涉相位的模拟
	cp phgt1 phgt0
	cp pdef1 pdef0
	cp pmask1 pmask0
	lin_comb_pt pt pmask1 phgt0 1 pdh1 1 phgt1 1 -0. 1. 1. 2 0 # phgt1= phgt0+pdh1
	lin_comb_pt pt pmask1 pdef0 1 pddef1 1 pdef1 1 -0. 1. 1. 2 0 # pdef1= pdef0+pddef1



